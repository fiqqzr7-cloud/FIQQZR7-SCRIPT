-- =========================
-- HIJAU DENYUT (ORION UI SAHAJA) - FIQQ FIXED
-- =========================

task.wait(0.5)

local TweenService = game:GetService("TweenService")
local CoreGui = game:GetService("CoreGui")

local GREEN_DARK = Color3.fromRGB(0, 120, 0)
local GREEN_LIGHT = Color3.fromRGB(0, 255, 0)

local tweenInfo = TweenInfo.new(
    1.2,
    Enum.EasingStyle.Sine,
    Enum.EasingDirection.InOut,
    -1,
    true
)

-- SEMAK sama ada UI Orion
local function IsOrionUI(obj)
    local parent = obj
    while parent do
        if parent.Name:lower():find("orion") then
            return true
        end
        -- STOP kalau masuk Topbar Roblox
        if parent.Name == "Topbar" or parent.Name == "RobloxGui" then
            return false
        end
        parent = parent.Parent
    end
    return false
end

local function PulseText(obj)
    if (obj:IsA("TextLabel") or obj:IsA("TextButton"))
        and obj.Text ~= ""
        and IsOrionUI(obj) then

        pcall(function()
            obj.TextColor3 = GREEN_DARK
            TweenService:Create(obj, tweenInfo, {
                TextColor3 = GREEN_LIGHT
            }):Play()
        end)
    end
end

-- UI sedia ada
for _, v in pairs(CoreGui:GetDescendants()) do
    PulseText(v)
end

-- UI baru
CoreGui.DescendantAdded:Connect(function(v)
    task.wait()
    PulseText(v)
end)
